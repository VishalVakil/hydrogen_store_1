{
  "buildCommand": "npx shopify hydrogen build --codegen",
  "outputDirectory": "dist",
  "framework": "hydrogen",
  "functions": {
    "dist/server/index.js": {
      "runtime": "@vercel/edge@1.1.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/server/index.js"
    }
  ]
}
